{
    "name": "hiraeth/json",
    "description": "JSON normalizer support for the Hiraeth Nano-Framework",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "keywords": [],
    "license": "MIT",
    "authors": [
        {
            "name": "Matthew J. Sahagian",
            "email": "matthew.sahagian@gmail.com",
            "role": "Lead Architect / Developer"
        }
    ],
    "require": {
        "dotink/json": "^1.0",
        "hiraeth/app": "^3.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.0",
        "phpunit/phpunit": "^10.0",
        "hiraeth/doctrine": "^3.0",
        "hiraeth/cache": "^3.0"
    },
    "autoload": {
        "classmap": ["src"]
    },
    "autoload-dev": {
        "classmap": ["test/src"]
    },
    "type": "opus-package",
    "extra": {
        "opus": {
            "hiraeth/app": {
                "config": ""
            }
        }
    },
    "config": {
        "allow-plugins": {
            "imarc/opus": true
        }
    }
}
